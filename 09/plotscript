"mergesort.sorted" with linespoints, \
"mergesort.partially_sorted" with linespoints, \
"mergesort.reversed" with linespoints, \
"mmergesort.random" with linespoints, \
"mmergesort.sorted" with linespoints, \
"mmergesort.partially_sorted" with linespoints, \
"mmergesort.reversed" with linespoints
